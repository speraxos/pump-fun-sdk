{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.nirholas/pump-fun-sdk",
  "title": "Pump SDK MCP Server",
  "description": "Pump SDK MCP Server â€” Solana token launchpad with bonding curves, vanity addresses, key management, and transaction building",
  "license": "MIT",
  "authors": [
    {
      "name": "nirholas",
      "url": "https://github.com/nirholas"
    }
  ],
  "repository": {
    "url": "https://github.com/nirholas/pump-fun-sdk",
    "source": "github"
  },
  "version": "1.0.0",
  "links": {
    "homepage": "https://github.com/nirholas/pump-fun-sdk",
    "bugs": "https://github.com/nirholas/pump-fun-sdk/issues"
  },
  "icons": [
    {
      "url": "https://raw.githubusercontent.com/nirholas/pump-fun-sdk/main/assets/milady.svg",
      "type": "svg"
    }
  ],
  "packages": [
    {
      "registryType": "npm",
      "identifier": "@nirholas/pump-fun-sdk",
      "version": "1.0.0",
      "runtimeHint": "npx",
      "transport": {
        "type": "stdio"
